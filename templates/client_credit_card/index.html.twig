{% extends 'base.html.twig' %}

{% block title %}ClientCreditCard index{% endblock %}

{% block body %}
    <h1>ClientCreditCard index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Number</th>
                <th>Client_id</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for client_credit_card in client_credit_cards %}
            <tr>
                <td>{{ client_credit_card.id }}</td>
                <td>{{ client_credit_card.Number }}</td>
                <td>{{ client_credit_card.clientId }}</td>
                <td>
